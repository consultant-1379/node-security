<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified"
	elementFormDefault="qualified">
	<xs:element name="Nodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Node" maxOccurs="unbounded" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:string" name="NodeFdn">
								<xs:annotation>
									<xs:documentation>
										Node name in any of these 2 format e.g. nodeName or NetworkElement=NetworkElementName
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element type="xs:string" name="SubAltName" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										The subject alternative name for the certificate.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element type="xs:string" name="SubAltNameType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										The subject alternative name for the certificate.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:choice>
								<xs:element name="EnableOMConfiguration1" minOccurs="0"
									maxOccurs="1">
									<xs:annotation>
										<xs:documentation>
											This will enable the IPSec for OM  of given node, if and only if IPSec for OM is disable. 
										</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element type="xs:boolean"
												name="removeTrustOnFailure" default="false">
												<xs:annotation>
													<xs:documentation>
														Remove the trust on failure
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string"
												name="trustedCertificateFilePath" minOccurs="0" maxOccurs="1">
												<xs:annotation>
													<xs:documentation>
														Provision to install operator provided certificate (Optional
														attribute. If node does not have certificate and this
														attribute is missing then configuration will fail)
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType" name="dnsServer1" minOccurs="0">
												<xs:annotation>
													<xs:documentation>
														The IP address of the primary DNS.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType" name="dnsServer2" minOccurs="0">
												<xs:annotation>
													<xs:documentation>
														The IP address of the secondary DNS.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType"
												name="ipAddressOaMInner"/>
											<xs:element type="xs:long" name="networkPrefixLength">
												<xs:annotation>
													<xs:documentation>
														Specifies the number of bits that are valid in the subnet address.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string" name="ipAccessHostEtId"/>
											<xs:element type="IpAddressType" name="defaultrouter0">
												<xs:annotation>
													<xs:documentation>
														The IP address of the Default Gateway
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType"
												name="ipAddressOaMOuter"/>
											<xs:element type="IpAddressType" name="remoteIpAddress">
												<xs:annotation>
													<xs:documentation>
														The remote IP address of the peer node.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string" name="remoteIpAddressMask"/>
											<xs:element type="IpAddressType" name="peerOaMIpAddress"/>
											<xs:element type="xs:string" name="peerIdentityIdFqdn" minOccurs="0"/>
											<xs:element type="xs:string" name="peerIdentityIdType">
												<xs:annotation>
													<xs:documentation>
														Value of this must be one of these "ALL" or "IP_V4_ADDRESS" or "FQDN"
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string" name="tsLocalIpAddressMask"/>
											<xs:element name="tsRemoteIpAddressRanges" maxOccurs="25">
												<xs:annotation>
													<xs:documentation>
														The remote IP address range that is part of the traffic selector which specifies the traffic allowed through the tunnel.
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="ipAddress" type="IpAddressType">
															<xs:annotation>
																<xs:documentation>
																	The remote IP address range that is part of the traffic selector which specifies the traffic allowed through the tunnel. This represents IP address .
																</xs:documentation>
															</xs:annotation>
														</xs:element>

														<xs:element name="mask" type="xs:string">
															<xs:annotation>
																<xs:documentation>
																	The remote IP address range that is part of the traffic selector which specifies the traffic allowed through the tunnel. This represents mask .
																</xs:documentation>
															</xs:annotation>
														</xs:element>

													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="ipSecTunnelAllowedTransforms">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="ipSecTunnelAllowedTransform" maxOccurs="10">
															<xs:annotation>
																<xs:documentation>
																	List of ipSecTunnelAllowedTransforms
																</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:all>
																	<xs:element type="ipSecTunnelEncryptionAlgorithm" name="encryptionAlgorithm" minOccurs="1">
																		<xs:annotation>
																			<xs:documentation>
																				encryptionAlgorithm in ipSecTunnelAllowedTransforms .
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>

																	<xs:element type="ipSecTunnelIntegrityAlgorithm" name="integrityAlgorithm" minOccurs="1">
																		<xs:annotation>
																			<xs:documentation>
																				integrityAlgorithm in ipSecTunnelAllowedTransforms .
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:all>
															</xs:complexType>
                                                        </xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="ikePeerAllowedTransforms">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="ikePeerAllowedTransform" maxOccurs="5">
															<xs:annotation>
																<xs:documentation>
																	List of ikePeerAllowedTransforms
																</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:all>
																	<xs:element type="diffieHellmanGroupType" name="diffieHellmanGroup" minOccurs="1">
																		<xs:annotation>
																			<xs:documentation>
																				diffieHellmanGroup in ikePeerAllowedTransforms .
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element type="ikePeerEncryptionAlgorithm" name="encryptionAlgorithm" minOccurs="1">
																		<xs:annotation>
																			<xs:documentation>
																				encryptionAlgorithm in ikePeerAllowedTransforms .
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element type="ikePeerIntegrityAlgorithm" name="integrityAlgorithm" minOccurs="1">
																		<xs:annotation>
																			<xs:documentation>
																				integrityAlgorithm in ikePeerAllowedTransforms .
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element type="pseudoRandomFunctionType" name="pseudoRandomFunction" minOccurs="1">
																		<xs:annotation>
																			<xs:documentation>
																				pseudoRandomFunction in ikePeerAllowedTransforms .
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:all>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element type="xs:int" name="vid" minOccurs="0" maxOccurs="1" />
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<!-- Configuration 2 is not supported currently and will be added as part of MR 55921 -->
								<xs:element name="EnableOMConfiguration2" minOccurs="0"
									maxOccurs="1">
									<xs:annotation>
										<xs:documentation>
											This will enable the IPSec for OM of given node, if and only if IPSec for OM is disable.
										</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element type="xs:boolean"
												name="removeTrustOnFailure" default="false">
												<xs:annotation>
												<xs:documentation>
														Remove the trust on failure Remove the trust on failure
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string"
												name="trustedCertificateFilePath">
												<xs:annotation>
												<xs:documentation>
														Provision to install operator provided certificate (Optional
														attribute. If node does not have certificate and this
														attribute is missing then configuration will fail)
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType" name="dnsServer1">
												<xs:annotation>
												<xs:documentation>
														The IP address of the primary DNS.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType" name="dnsServer2">
												<xs:annotation>
												<xs:documentation>
														The IP address of the secondary DNS.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType"
												name="ipAddressOaMInner"/>
											<xs:element type="xs:long" name="networkPrefixLength">
												<xs:annotation>
												<xs:documentation>
														Specifies the number of bits that are valid in the subnet address.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string" name="ipAccessHostEtRef"/>
											<xs:element type="IpAddressType" name="peerOaMIpAddress"/>
											<xs:element type="xs:string" name="peerIdentityIdFqdn"/>
											<xs:element type="xs:string" name="peerIdentityIdType"/>
											<xs:element type="xs:string" name="tsLocalIpAddressMask"/>
											<xs:element name="tsRemoteIpAddressRanges" maxOccurs="25">
												<xs:annotation>
													<xs:documentation>
														The remote IP address range that is part of the traffic selector which specifies the traffic allowed through the tunnel.
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="ipAddress" type="IpAddressType" >
															<xs:annotation>
																<xs:documentation>
																	The remote IP address range that is part of the traffic selector which specifies the traffic allowed through the tunnel. This represents IP address .
																</xs:documentation>
															</xs:annotation>														
														</xs:element>
														
														<xs:element name="mask" type="xs:string">
															<xs:annotation>
																<xs:documentation>
																	The remote IP address range that is part of the traffic selector which specifies the traffic allowed through the tunnel. This represents mask .
																</xs:documentation>
															</xs:annotation>
														</xs:element>

													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element type="xs:string" name="vid" minOccurs="0" maxOccurs="1"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="DisableOMConfiguration" minOccurs="0"
									maxOccurs="1">
									<xs:annotation>
										<xs:documentation>
											This will disable the IPSec for OM  of given node, if and only if IPSec for OM is enable.
										</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element type="xs:boolean" name="removeCert"
												default="false">
												<xs:annotation>
												<xs:documentation>
														If this removeCert value is true then node certificate will
														be removed after disabling OM configuration
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="removeTrust" minOccurs="0">
												<xs:annotation>
												<xs:documentation>
														If this value is true then node trusted certificate will be
														removed after disabling OM Configuration
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
												<xs:sequence>
												<xs:element type="xs:long" name="serialNumber"
												minOccurs="1" maxOccurs="1">
												<xs:annotation>
												<xs:documentation>
															A string of decimal digits containing the unique serial number of the certificate assigned by the certificate issuer.
														</xs:documentation>
												</xs:annotation>
												</xs:element>
												<xs:element type="xs:string" name="issuer"
												minOccurs="1" maxOccurs="1">
												<xs:annotation>
												<xs:documentation>
															This is a string representation of the X.509 name of the authority that has issued the certificate. It is of the same type as the distinguished name used in the subject field of a certificate request. It is encoded as FIELDNAME=VALUE where fields are separated with a comma (,) character.
															This string cannot be empty. Maximum string length is 127. If the string is longer, trailing FIELDNAME=VALUE pairs will not be used in the comparison.
															Example: C=SE, L=LI, O=Ericsson AB, OU=FJK/LO, CN=PKS 2005-08-30 
														</xs:documentation>
												</xs:annotation>
												</xs:element>
												</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element type="IpAddressType" name="dnsServer1" minOccurs="0">
												<xs:annotation>
												<xs:documentation>
														The IP address of the primary DNS.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType" name="dnsServer2" minOccurs="0">
												<xs:annotation>
												<xs:documentation>
														The IP address of the secondary DNS.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType"
												name="ipAddressOaMOuter"/>
											<xs:element type="IpAddressType" name="defaultRouter0">
												<xs:annotation>
												<xs:documentation>
														The IP address of the Default Gateway.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:long" name="networkPrefixLength">
												<xs:annotation>
												<xs:documentation>
														Specifies the number of bits that are valid in the subnet address.
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="IpAddressType" name="remoteIpAddress"
											/>				
											<xs:element type="xs:string" name="remoteIpAddressMask" />
											<xs:element type="xs:int" name="vid" minOccurs="0" maxOccurs="1"/>								
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="InstallTrustCertificates" minOccurs="0"
									maxOccurs="1">
									<xs:annotation>
										<xs:documentation>
											This is to install trust certificate for given node. 
										</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element type="xs:boolean"
												name="removeTrustOnFailure" default="false">
												<xs:annotation>
												<xs:documentation>
													Remove the trust on failure
												</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string"
												name="trustedCertificateFilePath">
												<xs:annotation>
												<xs:documentation>
												Provision to install operator provided certificate
											</xs:documentation>
												</xs:annotation>
											</xs:element>

										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<!-- Added SiteBasic tag in choice TORF-103962-->
								<xs:element name="SiteBasic">
									<xs:complexType>
										<xs:choice maxOccurs="unbounded">
											<xs:element name="Format">
												<xs:annotation>
													<xs:documentation>Contains information about the revision
														of the schema.
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attribute name="revision" use="required">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="K" />
																<xs:enumeration value="D" />
																<xs:enumeration value="J" />
																<xs:enumeration value="T" />
																<xs:enumeration value="U" />
																<xs:enumeration value="V" />
																<xs:enumeration value="W" />
																<xs:enumeration value="X" />
																<xs:enumeration value="Y" />
																<xs:enumeration value="Z" />
																<xs:enumeration value="AA" />
																<xs:enumeration value="AB" />
																<xs:enumeration value="AC" />
																<xs:enumeration value="AD" />
																<xs:enumeration value="AE" />
																<xs:enumeration value="AF" />
																<xs:enumeration value="AG" />
																<xs:enumeration value="AH" />
																<xs:enumeration value="AJ" />
																<xs:enumeration value="AK" />
																<xs:enumeration value="AL" />
																<xs:enumeration value="AM" />
																<xs:enumeration value="AN" />
																<xs:enumeration value="AO" />
																<xs:enumeration value="AP" />
																<xs:enumeration value="AR" />
																<xs:enumeration value="AS" />
																<xs:enumeration value="AT" />
																<xs:enumeration value="AU" />
																<xs:enumeration value="AV" />
																<xs:enumeration value="AX" />
															</xs:restriction>
														</xs:simpleType>
													</xs:attribute>
												</xs:complexType>
											</xs:element>
											<xs:element name="Ip">
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element name="IpRoutingTable" maxOccurs="1">
															<xs:complexType>
																<xs:choice>
																	<xs:element name="StaticRoutes" minOccurs="0"
																		maxOccurs="200">
																		<xs:complexType>
																			<xs:attribute name="ipAddress" type="ipv4oIpv6Type"
																				use="required" />
																			<xs:attribute name="networkMask" type="netMask"
																				use="required" />
																			<xs:attribute name="nextHopIpAddr" type="ipv4oIpv6Type"
																				use="required" />
																			<xs:attribute name="redistribute" type="TrueOrFalse"
																				use="required" />
																			<xs:attribute name="routeMetric" type="xs:int"
																				use="required" />
																		</xs:complexType>
																	</xs:element>
																</xs:choice>
																<xs:attribute name="userLabel" type="xs:string"
																	default="" />
															</xs:complexType>
														</xs:element>
														<xs:element name="IpHostLink" maxOccurs="1">
															<xs:complexType>
																<xs:attribute name="ipAddress" type="ipv4oIpv6Type"
																	use="optional" />
																<xs:attribute name="ipHostLinkId" type="xs:string"
																	use="required" />
																<xs:attribute name="ipInterfaceMoRef" type="xs:string"
																	use="required" />
																<xs:attribute name="userLabel" type="xs:string"
																	default="" />
															</xs:complexType>
														</xs:element>
														<xs:element name="oaMAlternativeAccessSource"
															minOccurs="0">
															<xs:complexType>
																<xs:attribute name="ipAddress" type="ipv4oIpv6Type"
																	use="required" />
																<xs:attribute name="networkMask" type="netMask"
																	use="required" />
															</xs:complexType>
														</xs:element>
													</xs:choice>
													<xs:attribute name="dscp" use="optional">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:minInclusive value="0" />
																<xs:maxInclusive value="63" />
															</xs:restriction>
														</xs:simpleType>
													</xs:attribute>
													<xs:attribute name="dnsServer1" type="ipv4oIpv6Type"
														use="optional" />
													<xs:attribute name="dnsServer2" type="ipv4oIpv6Type"
														use="optional" />
													<xs:attribute name="dnsServer3" type="ipv4oIpv6Type"
														use="optional" />
													<xs:attribute name="oaMAlternativeAccess"
														type="TrueOrFalse" use="optional" />
												</xs:complexType>
											</xs:element>
											<xs:element name="ENodeBFunction">
												<xs:annotation>
													<xs:documentation>UpIpAccessHostEtRef is mandatory if more
														than one IpAccessHostEt is configured on the node, i.e. if
														IpSec is used.
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element name="RbsConfiguration">
															<xs:complexType>
																<xs:choice maxOccurs="unbounded">
																	<xs:element name="WantedPosition" minOccurs="0"
																		maxOccurs="1">
																		<xs:complexType>
																			<xs:attribute name="latitude" type="xs:string"
																				use="required" />
																			<xs:attribute name="longitude" type="xs:string"
																				use="required" />
																			<xs:attribute name="altitude" type="xs:string"
																				use="required" />
																			<xs:attribute name="tolerance" type="xs:int"
																				use="optional" />
																		</xs:complexType>
																	</xs:element>
																</xs:choice>
																<xs:attribute name="ossCorbaNameServer"
																	type="xs:string" use="required" />
																<xs:attribute name="nodeIdType" type="NodeIdTypeEnum"
																	default="FDN" use="optional" />
																<xs:attribute name="nodeIdValue" type="xs:string"
																	default="" use="optional" />
															</xs:complexType>
														</xs:element>
													</xs:choice>
													<xs:attribute name="upIpAccessHostRef" type="xs:string"
														use="optional" />
													<xs:attribute name="timeAndPhaseSynchCritical"
														type="TrueOrFalse" use="optional" />
													<xs:attribute name="timeAndPhaseSynchAlignment"
														type="TrueOrFalse" use="optional" />
													<xs:attribute name="timePhaseMaxDeviation"
														type="timePhaseMaxDeviationValue" use="optional" />
													<xs:attribute name="timePhaseMaxDeviationCdma2000"
														type="timePhaseMaxDeviationValue" use="optional" />
													<xs:attribute name="timePhaseMaxDeviationMbms"
														type="timePhaseMaxDeviationValue" use="optional" />
													<xs:attribute name="timePhaseMaxDeviationOtdoa"
														type="timePhaseMaxDeviationValue" use="optional" />
													<xs:attribute name="timePhaseMaxDeviationSib16"
														type="timePhaseMaxDeviationValue" use="optional" />
													<xs:attribute name="timePhaseMaxDeviationTdd"
														type="timePhaseMaxDeviationValue" use="optional" />
